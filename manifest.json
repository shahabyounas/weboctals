    "share_target": {
        "action": "/share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "protocol_handlers": [
        {
            "protocol": "web+weboctals",
            "url": "/handler?url=%s"
        }
    ],
    "prefer_related_applications": false,
    "related_applications": [
        {
            "platform": "webapp",
            "url": "https://weboctals.com/manifest.json"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "handle_links": "preferred",
    "scope_extensions": [
        {
            "origin": "https://weboctals.com"
        }
    ],
    "file_handlers": [
        {
            "action": "/file-handler",
            "accept": {
                "application/json": [
                    ".json"
                ],
                "text/plain": [
                    ".txt",
                    ".md"
                ],
                "application/pdf": [
                    ".pdf"
                ]
            }
        }
    ]
}